<script>
  document.addEventListener("DOMContentLoaded", () => {
    const accordionItems = document.querySelectorAll("[data-instant-type='accordion-item']");

    accordionItems.forEach(item => {
      const header = item.querySelector("[data-instant-type='accordion-header']");
      const content = item.querySelector("[data-instant-type='accordion-content']");

      header.addEventListener("click", () => {
        const isOpen = item.getAttribute("data-state") === "open";

        // Close all accordion items if multi-open is disabled
        const parentContainer = item.closest("[data-instant-type='accordion-container']");
        if (!parentContainer.dataset.isMultiOpenEnabled) {
          parentContainer.querySelectorAll("[data-instant-type='accordion-item']").forEach(i => {
            i.setAttribute("data-state", "closed");
            i.querySelector("[data-instant-type='accordion-content']").style.height = "0";
          });
        }

        // Toggle the current item
        if (isOpen) {
          item.setAttribute("data-state", "closed");
          content.style.height = "0";
        } else {
          item.setAttribute("data-state", "open");
          content.style.height = content.scrollHeight + "px";
        }
      });

      // Set initial content height
      if (item.getAttribute("data-state") === "open") {
        content.style.height = content.scrollHeight + "px";
      } else {
        content.style.height = "0";
      }
    });
  });
</script>

